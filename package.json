{
  "name": "AudiblePrototypeServer",
  "version": "1.5.0",
  "description": "HIG Holder",
  "main": "dist/engine.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" node ./devserver",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "watch": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\"",
    "postbuild": "cp -R dist docs"
  },
  "dependencies": {
    "AudibleUITags": "file:vends/AudibleUITags",
    "babel": "^5.6.14",
    "babel-plugin-typecheck": "^1.2.0",
    "body-parser": "~1.12.4",
    "classnames": "^2.1.2",
    "compression": "^1.5.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.12.4",
    "fastclick": "^1.0.6",
    "history": "1.17.0",
    "jsonfile": "^2.0.1",
    "lodash": "^3.9.3",
    "mobile-detect": "^1.2.1",
    "piping": "^0.2.0",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-jsx": "^0.13.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "serve-favicon": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "5.8.20",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.20",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "concurrently": "0.1.1",
    "del": "^1.2.0",
    "eslint-plugin-react": "^2.3.0",
    "file-loader": "^0.8.4",
    "glob": "^5.0.14",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-gzip": "^1.2.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "json-loader": "0.5.2",
    "react-hot-loader": "1.2.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "1.10.5",
    "webpack-dev-server": "1.10.1"
  }
}
